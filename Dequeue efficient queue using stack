import java.util.*;
public class Main {
    public static void main(String args[]) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		Stack<Integer>st=new Stack<>();
		for(int i=0;i<n;i++)
		{
			st.push(i);
		}
		Stack<Integer>st2=new Stack<>();
		while(!st.isEmpty())
		{
			st2.push(st.pop());
		}
		while(!st2.isEmpty())
		{
			System.out.print(st2.pop()+" ");

		}
    }
}
